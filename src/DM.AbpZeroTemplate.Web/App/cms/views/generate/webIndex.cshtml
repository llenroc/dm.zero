<div ng-controller="cms.views.generate.webIndex as vm">
    <div class="row margin-bottom-5">
        <div class="col-xs-12">
            <div class="page-head">
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-12">
            <div class="portlet light">
                <div class="portlet-title">
                    <div class="caption">
                        <span class="caption-subject bold uppercase">
                            @L("GenerateIndex")
